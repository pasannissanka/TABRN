{
  "name": "TL;RL",
  "version": "0.0.0",
  "description": "An awesome new browser extension",
  "homepage_url": "https://github.com/awesome-templates/browser-extension-template",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "icons": {
    "128": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "cookies",
    "http://localhost:4001/"
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "background": {
    "scripts": ["background.ts"]
  },
  "browser_action": {
    "default_title": "Read Later...",
    "default_popup": "popups/cookie_consent.html"
  },
  "web_accessible_resources": ["popups/*.html"]
}
